{
  "name": "@web3-blocks/dapp-ui",
  "version": "1.0.3",
  "description": "The Universal and TypeScript-first Web3 abstraction layer that provides a unified provider, modular network hooks, and chain-specific utilities for building decentralized applications.",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "author": "thelastofinusa",
  "scripts": {
    "prepare": "husky",
    "genotype": "node src/scripts/genotype.mjs",
    "build": "node src/scripts/genotype.mjs && tsup",
    "frontend": "node src/scripts/genotype.mjs && tsup && cd example && npm link @web3-blocks/dapp-ui && clear && pnpm run dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "homepage": "https://github.com/web3-blocks/dapp-ui-core#readme",
  "bugs": {
    "url": "https://github.com/web3-blocks/dapp-ui-core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web3-blocks/dapp-ui-core.git"
  },
  "keywords": [
    "web3-blocks",
    "dapp-ui",
    "web3",
    "wagmi",
    "viem",
    "dapp",
    "hooks",
    "wallet",
    "blockchain",
    "react",
    "ethereum",
    "connect",
    "client"
  ],
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@tanstack/react-query": "^5.90.8",
    "@types/react": "^19.2.4",
    "ethers": "^6.15.0",
    "husky": "^9.1.7",
    "jsdom": "^24.1.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "viem": "^2.39.0",
    "vitest": "^1.6.1",
    "wagmi": "^2.13.0"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "wagmi": "^2",
    "viem": "^2",
    "ethers": "^6",
    "@tanstack/react-query": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    },
    "wagmi": {
      "optional": false
    },
    "viem": {
      "optional": false
    },
    "ethers": {
      "optional": false
    },
    "@tanstack/react-query": {
      "optional": false
    }
  },
  "engines": {
    "node": ">=18"
  }
